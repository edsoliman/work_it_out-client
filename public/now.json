{
    "version": 2,
    "alias": "eds-workitout-app",
    "name": "workitout-app",
    "routes": [
        {
          "src": "^/static/(.*)",
          "dest": "/static/$1"
        },
        {
          "src": ".*",
          "dest": "/index.html"
        }
    ],
    "build": {
      "env": {
        "FIREBASE_API": "@react_app_firebase_api_key",
        "APP_API_KEY": "@react_app_api_key"
      }
    }
  }