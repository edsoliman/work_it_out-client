{
    "version": 2,
    "alias": "eds-workitout-app",
    "name": "workitout-app",
    "routes": [
        {
          "src": "^/static/(.*)",
          "dest": "/static/$1"
        },
        {
          "src": ".*",
          "dest": "/index.html"
        }
    ],
    "env": {
      "REACT_APP_FIREBASE_API_KEY": "@firebase-api-key",
      "REACT_APP_API_KEY": "@app-api-key"
    },
    "build": {
      "env": {
        "REACT_APP_FIREBASE_API_KEY": "@firebase-api-key",
        "REACT_APP_API_KEY": "@app-api-key"
      }
    }
  }